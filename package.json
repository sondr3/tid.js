{
  "name": "tid.js",
  "version": "1.1.0",
  "description": "A tiny and stupidly simple library to display how long ago something was posted",
  "keywords": [
    "frontend",
    "datetime",
    "time",
    "timeago"
  ],
  "homepage": "https://tid.eons.io",
  "repository": "sondr3/tid.js",
  "author": "Sondre Nilsen",
  "license": "MIT",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "scripts": {
    "optimize": "terser --compress --mangle -o tid.min.js -- tid.js",
    "bundle": "cp tid.js tid.min.js ../../sites/tid.eons/content",
    "build": "yarn optimize && yarn bundle"
  },
  "files": [
    "README.md",
    "LICENSE",
    "tid.js",
    "tid.min.js"
  ],
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.2",
    "terser": "^3.14.1"
  }
}
