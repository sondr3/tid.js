{
  "name": "tid.js",
  "version": "1.2.0",
  "description": "A tiny and stupidly simple library to display how long ago something was posted",
  "keywords": [
    "frontend",
    "datetime",
    "time",
    "timeago"
  ],
  "homepage": "https://tid.eons.io",
  "repository": "sondr3/tid.js",
  "author": "Sondre Nilsen",
  "license": "MIT",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "scripts": {
    "optimize": "terser --compress --mangle -o tid.min.js -- tid.js",
    "build": "yarn optimize"
  },
  "files": [
    "README.md",
    "LICENSE",
    "tid.js",
    "tid.min.js"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "terser": "^4.6.6"
  }
}
